<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="mg.fjkm.ankadifotsy.bo">
	<class name="AntonAsa" table="ANTONASA">
		<id	column="ANTONASA_ID"	name="id">
			<generator class="increment"/>
		</id>

		<property name="intitule" column="ANTONASA_INTITULE" not-null="true"/>
		<property name="description" column="ANTONASA_DESCR" />

	</class>
</hibernate-mapping>
